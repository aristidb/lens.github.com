---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>The Lens Library</title>
    <link>http://lens.github.com/</link>
    <atom:link href="http://lens.github.com/rss.xml" rel="self" type="application/rss+xml" />
    <description>The Lens Library</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      {% if post.link != null %}
      <link>{{ post.link }}</link>
      {% else %}
      <link>http://lens.github.com{{ post.url }}</link>
      {% endif %}
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Edward Kmett (ekmett@gmail.com)</author>
      {% if post.link != null %}
      <guid>{{ post.link }}</guid>
      {% else %}
      <guid>http://lens.github.com{{ post.id }}</guid>
      {% endif %}
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel>
</rss>
